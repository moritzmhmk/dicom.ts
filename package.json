{
  "name": "dicom.ts",
  "version": "1.0.4",
  "description": "A small, fast, typescript DICOM renderer",
  "author": "wearemothership",
  "keywords": [
    "dicom",
    "parser",
    "renderer",
    "canvas",
    "typescript",
    "glsl",
    "image",
    "jpeg",
    "jpeg2000",
    "jpeglossless",
    "jpegls",
    "RLE"
  ],
  "email": "nick@wearemothership.com",
  "license": "MIT",
  "repository": "wearemothership/dicom.ts",
  "homepage": "https://wearemothership.github.io/dicom.ts",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "exports": {
    "require": "./dist/index.js",
    "default": "./dist/index.modern.js"
  },
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "rimraf dist && microbundle --no-compress -f modern,cjs",
    "start": "rimraf dist && microbundle watch --no-compress -f modern,cjs",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "jest .",
    "test:coverage": "jest . --coverage",
    "predeploy": "cd example-vs-cornerstone && npm install && SKIP_PREFLIGHT_CHECK=true npm run build",
    "deploy": "gh-pages -d example-vs-cornerstone/build"
  },
  "dependencies": {
    "@types/node": "^14.14.31",
    "@wearemothership/dicom-character-set": "^1.0.4-opt.1",
    "charls": "github:moritzmhmk/charls-js",
    "jpeg-lossless-decoder-js": "^v2.0.4",
    "pako": "^1.0.10",
    "raw.macro": "^0.4.2",
    "sha1": "^1.1.1",
    "twgl.js": "^4.19.1",
    "typescript": "^4.2.3"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.13.8",
    "@babel/plugin-proposal-optional-chaining": "^7.13.8",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-typescript": "^7.13.0",
    "@types/sha1": "^1.1.3",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^27.2.4",
    "babel-plugin-macros": "^3.0.1",
    "blob-util": "^2.0.2",
    "canvas": "^2.8.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.22.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.2",
    "gh-pages": "^2.2.0",
    "jest": "^27.2.4",
    "microbundle": "^0.14.2",
    "node-canvas-webgl": "^0.2.6",
    "node-fetch": "^2.6.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "rimraf": "^3.0.2"
  },
  "jest": {
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    },
    "testPathIgnorePatterns": [
      "example",
      "example-vs-cornerstone"
    ]
  },
  "files": [
    "dist"
  ]
}
